{
    "name": "hpcc-docs",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "clean": "rimraf -g .vitepress/cache .vitepress/dist dist node_modules/.vite *.tsbuildinfo",
        "dev": "vite --port 3000",
        "dev-docs": "vitepress dev --port 4000",
        "dev-test": "vitest",
        "build": "tsc -b && vite build",
        "build-docs": "vitepress build",
        "preview": "vite preview",
        "preview-docs": "vitepress preview",
        "lint": "eslint .",
        "update": "npx -y npm-check-updates -u -t minor",
        "update-major": "npx -y npm-check-updates -u"
    },
    "dependencies": {
        "@hpcc-js/observablehq-compiler": "^1.3.0",
        "@observablehq/runtime": "^5.9.9",
        "d3-fetch": "3.0.1"
    },
    "devDependencies": {
        "@eslint/js": "8.57.1",
        "@shikijs/markdown-it": "1.22.0",
        "eslint": "8.57.1",
        "eslint-gitignore": "0.1.0",
        "globals": "15.10.0",
        "markdown-it": "14.1.0",
        "rimraf": "6.0.1",
        "typescript": "5.6.2",
        "typescript-eslint": "8.8.0",
        "vite": "5.4.8",
        "vitepress": "1.3.4",
        "vitest": "2.1.2"
    }
}