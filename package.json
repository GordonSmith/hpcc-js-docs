{
    "name": "hpcc-docs",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "clean": "rimraf -g .vitepress/cache .vitepress/dist dist *.tsbuildinfo",
        "dev": "vite --port 3000",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "docs-dev": "vitepress dev --port 4000",
        "docs-build": "vitepress build",
        "docs-preview": "vitepress preview",
        "test-dev": "vitest",
        "update": "npx -y npm-check-updates -u -t minor",
        "update-major": "npx -y npm-check-updates -u"
    },
    "dependencies": {
        "@hpcc-js/observablehq-compiler": "1.3.0",
        "d3-fetch": "3.0.1",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@eslint/js": "8.57.1",
        "@observablehq/runtime": "5.9.9",
        "@observablehq/stdlib": "5.8.8",
        "@shikijs/markdown-it": "1.21.0",
        "@types/react": "18.3.11",
        "@types/react-dom": "18.3.0",
        "@vitejs/plugin-react-swc": "3.7.1",
        "eslint": "8.57.1",
        "eslint-gitignore": "0.1.0",
        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-react-refresh": "0.4.12",
        "globals": "15.10.0",
        "markdown-it": "14.1.0",
        "rimraf": "6.0.1",
        "typescript": "5.6.2",
        "typescript-eslint": "8.8.0",
        "vite": "5.4.8",
        "vitepress": "1.3.4",
        "vitest": "2.1.2"
    }
}